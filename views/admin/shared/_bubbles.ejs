    <div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
			<h3 class="text-success text-right">
				NodeJS bubbles
			</h3>
            <button type="button" class="btn btn-success btn-block btn-lg">
				create new
			</button>
			<div class="row">
                <% NodeBubbles.forEach(function(el){ %>

                    <div class="col-md-4">
                        <div class="thumbnail">
                            <img alt="Bootstrap Thumbnail First" src="https://www.visualstudio.com/wp-content/uploads/2016/06/Nodejs-2-562x309@2x-op.png" />
                            <div class="caption">
                                <h3>
                                    <%= el.BubbleName %> 
                                </h3>
                                    <%= el.main %>
                                    <%= el.IsRunning() =>
                                    <button type="button" class="btn btn-danger pmd-ripple-effect">down<span class="badge"></span></button>
                                <p>
                                    <%=  %>
                                </p>
                                <p>
                                    <a class="btn btn-primary" href="#">Action</a> <a class="btn" href="#">Action</a>
                                </p>
                            </div>
                        </div>
                    </div>
                <%}); %>
			</div>
		</div>
	</div>
</div>

